<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AutoLineView">
        <attr name="horizontalSpace" format="dimension|reference"/>
        <attr name="vertivalSpace" format="dimension|reference"/>
    </declare-styleable>

    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="MyLinearLayout">
        <attr name="content" format="string"/>
        <attr name="title" format="string"/>
        <attr name="money" format="string"/>
        <attr name="src" format="reference"/>
        <attr name="money_layout" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MyEditLayout">
        <attr name="my_info_title" format="string"/>
        <attr name="my_info_content" format="string"/>
        <attr name="my_info_number_type" format="boolean"/>
    </declare-styleable>
</resources>